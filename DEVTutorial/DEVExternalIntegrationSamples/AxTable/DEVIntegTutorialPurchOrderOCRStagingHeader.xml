			<Method>
				<Name>InvoiceRoundingAmount</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display Amount InvoiceRoundingAmount()
    {
        DEVIntegTutorialPurchOrderOCRStagingLine  stagingLine;
        select sum(RoundingAmount) from stagingLine
            where stagingLine.HeaderRecId == this.RecId;

        return stagingLine.RoundingAmount ;
    }

]]></Source>
