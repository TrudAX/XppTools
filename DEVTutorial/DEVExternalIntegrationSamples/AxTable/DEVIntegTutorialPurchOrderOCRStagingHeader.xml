			<Method>
				<Name>getPurchTable</Name>
				<Source><![CDATA[
    PurchTable  getPurchTable(boolean _forUpdate = false)
    {
        PurchTable      purchTable;
        CompanyId       companyId;
        ;
        companyId = curExt();
        if (companyId)
        {
            changecompany(companyId)
            {
                purchTable = PurchTable::find(this.PurchId, _forUpdate);
            }
        }
        return purchTable;
    
    }

]]></Source>
