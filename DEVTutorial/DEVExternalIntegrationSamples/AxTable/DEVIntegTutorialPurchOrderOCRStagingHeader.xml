			<Method>
				<Name>InvoiceTotalAmountLines</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display Amount InvoiceTotalAmountLines() 
    {
        DEVIntegTutorialPurchOrderOCRStagingLine  stagingLine;
        select sum(TotalAmount) from stagingLine
            where stagingLine.HeaderRecId == this.RecId;

        return stagingLine.TotalAmount + this.ChargeValueFile;
    }

]]></Source>
