						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            CompanyId       companyId;
            Args            args;
            MenuFunction    mf;
            ;
        
            if (StagingHeader.PurchId && DEVIntegMessageTable.CompanyId)
            {
                companyId = DEVIntegMessageTable.CompanyId;
                changecompany(companyId)
                {
                    if (StagingHeader.getPurchTable().RecId)
                    {
                        args = new Args();
                        args.record(StagingHeader.getPurchTable());
                        mf = new MenuFunction(menuitemDisplayStr(PurchTableDetails), MenuItemType::Display);
                        mf.run(args);
                    }
                }
            }
        }

]]></Source>
