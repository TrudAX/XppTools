        /// </summary>
        public void clicked()
        {
            try
            {
                ttsbegin;
                DEVIntegMessageTable.selectForUpdate(true);
                DEVIntegMessageTable.IsParsed = NoYes::No;
                DEVIntegMessageTable.update();
                ttscommit;

                DEVIntegMessageProcessor messageProcessor = DEVIntegMessageProcessor::construct(DEVIntegMessageTable.messageTypeTable());
                messageProcessor.processMessage(DEVIntegMessageTable);
            }
            finally
            {
                DEV::dsResearch(DEVIntegMessageTable);
            }
        }

]]></Source>
