		<Methods>
			<Method>
				<Name>clearMessageLog</Name>
				<Source><![CDATA[
    public static void clearMessageLog(RefRecId _messageTableRecId)
    {
        DEVIntegErrorLog      errorLog;
        ttsbegin;
        delete_from errorLog
            where errorLog.MessageRefRecId == _messageTableRecId;
        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>insertErrorByMessage</Name>
				<Source><![CDATA[
    public static void insertErrorByMessage(DEVIntegMessageTable _messageTable, int _infoCnt, Exception  _exception)
    {
        DEVIntegErrorLog      errorLog;

        ttsbegin;

        container taskInfolog = conNull();
        if (infologLine() > _infoCnt)
        {
            taskInfolog = infolog.copy(_infoCnt + 1, infologLine());
        }
        errorLog.clear();
        errorLog.Exception      = _exception;
        errorLog.InfologData    = taskInfolog;
        if (taskInfolog != conNull())
        {
            errorLog.LogText        = info::infoCon2Str(taskInfolog);
            errorLog.MessageRefRecId        = _messageTable.RecId;
            errorLog.insert();
        }
        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>existForMessageTable</Name>
				<Source><![CDATA[
    public static boolean existForMessageTable(DEVIntegMessageRefRecId  _refRecId)
    {
        return (select firstonly RecId from DEVIntegErrorLog where DEVIntegErrorLog.MessageRefRecId == _refRecId).RecId != 0;
    }

]]></Source>
			</Method>
