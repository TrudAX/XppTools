			<Method>
				<Name>isHoldAvailiable</Name>
				<Source><![CDATA[
    public boolean isHoldAvailiable()
    {
        boolean res;
        if (this.Status == DEVIntegMessageStatus::Processing ||
            this.Status == DEVIntegMessageStatus::Ready      ||
            this.Status == DEVIntegMessageStatus::Error)
        {
            res = true;
        }
        return res;
    }

]]></Source>
