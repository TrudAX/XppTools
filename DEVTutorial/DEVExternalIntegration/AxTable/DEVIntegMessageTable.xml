			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    public void update()
    {
        if (this.ProcessingStartDateTime && this.ProcessingEndDateTime)
        {
            this.Duration = int642int(DateTimeUtil::getDifference(this.ProcessingEndDateTime, this.ProcessingStartDateTime));
        }
        super();
    }

]]></Source>
