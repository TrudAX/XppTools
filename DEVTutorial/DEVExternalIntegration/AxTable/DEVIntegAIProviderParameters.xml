				<Source><![CDATA[
    public static DEVIntegAIProviderParameters find(DEVIntegAIProviderId  _providerId,
                                                    Description           _parameterName,
                                                    boolean               _forUpdate = false)
    {
        DEVIntegAIProviderParameters integAIProviderParameters;
        ;
        if (_providerId)
        {
            integAIProviderParameters.selectForUpdate(_forUpdate);
            
            select firstonly integAIProviderParameters
                where integAIProviderParameters.ProviderId     == _providerId &&
                      integAIProviderParameters.ParameterName  == _parameterName;
        }
        
        return integAIProviderParameters;
    }

]]></Source>
