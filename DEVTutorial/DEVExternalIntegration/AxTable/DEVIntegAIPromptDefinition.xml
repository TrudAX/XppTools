]]></Declaration>
		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static DEVIntegAIPromptDefinition find(DEVIntegAIPromptDefinitionId  _promptDefinitionId,
                                                  boolean                       _forUpdate = false)
    {
        DEVIntegAIPromptDefinition integAIPromptDefinition;
        ;
        if (_promptDefinitionId)
        {
            integAIPromptDefinition.selectForUpdate(_forUpdate);
            
            select firstonly integAIPromptDefinition
                where integAIPromptDefinition.PromptDefinitionId  == _promptDefinitionId;
        }
        
        return integAIPromptDefinition;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getProvider</Name>
				<Source><![CDATA[
    public DEVIntegAIProvider getProvider()
    {
        DEVIntegAIProvider provider;
        ;
        
        provider = DEVIntegAIProvider::find(this.ProviderId);
        
        if (!provider.RecId)
        {
            throw error(strFmt("Provider with ID '%1' not found", this.ProviderId));
        }
        
        return provider;
    }

]]></Source>
			</Method>
