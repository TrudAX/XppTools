    public static DEVIntegAIProvider find(DEVIntegAIProviderId  _providerId,
                                          boolean               _forUpdate = false)
    {
        DEVIntegAIProvider integAIProvider;
        ;
        if (_providerId)
        {
            integAIProvider.selectForUpdate(_forUpdate);
            
            select firstonly integAIProvider
                where integAIProvider.ProviderId  == _providerId;
        }
        
        return integAIProvider;
    }

