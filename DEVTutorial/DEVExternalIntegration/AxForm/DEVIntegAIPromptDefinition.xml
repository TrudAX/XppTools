					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            TestResultStringControl.text('');
            TestResultStringStatistics.text('');

            DEVIntegAIProviderBase   providerBase = DEVIntegAIProviderBase::constructFromPromptDefinition(DEVIntegAIPromptDefinition);
            DEVIntegAIPromptDefinitionTestData  promptDefinitionTestData;
            select firstonly promptDefinitionTestData
                where promptDefinitionTestData.PromptDefinitionId == DEVIntegAIPromptDefinition.PromptDefinitionId;

            providerBase.setFileContainer(promptDefinitionTestData.FileName, promptDefinitionTestData.FileData);
            
            str res = providerBase.callAPIText();

            TestResultStringControl.text(res);

            DEVIntegAICallStatistics statistics = providerBase.getStatistics();
            TestResultStringStatistics.text(statistics.getFormattedStatistics());

        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>DownloadResultButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            super();
            File::SendStringAsFileToUser(TestResultStringControl.text() , 
                strFmt('AIPrompt_%1.txt', DEVIntegAIPromptDefinition.PromptDefinitionId));
        }

]]></Source>
					</Method>
